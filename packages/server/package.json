{
  "name": "@template/server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test": "export NODE_ENV=test PORT=3300 && mocha --timeout 2000 -r ts-node/register 'tests/**/*.test.ts' --exit",
    "coverage": "nyc --reporter=lcov --report-dir=coverage mocha --timeout 2000 -r ts-node/register 'tests/**/*.test.ts' --exit",
    "prod": "node ./build/app.js",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "eslint": "eslint './src/**/*.{ts,tsx}' --fix",
    "start": "NODE_ENV=development nodemon --watch 'src/**/*.ts' --exec 'ts-node --files' src/app.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@template/common": "0.1.0",
    "bcryptjs": "^2.4.3",
    "config": "^3.3.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.15",
    "swagger-jsdoc": "^6.0.1",
    "swagger-ui-express": "^4.1.6",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/config": "^0.0.38",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/express-validator": "^3.0.0",
    "@types/gravatar": "^1.8.1",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mocha": "^8.2.1",
    "@types/power-assert": "^1.5.3",
    "@types/supertest": "^2.0.10",
    "@types/swagger-jsdoc": "^6.0.0",
    "@types/swagger-ui-express": "^4.1.2",
    "eslint": "^7.19.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "power-assert": "^1.6.1",
    "supertest": "^6.1.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
